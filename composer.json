{
  "name": "user/library-system",
  "description": "Laravel 10 Library Management System scaffold",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "stripe/stripe-php": "^18.2",
    "laravel/sanctum": "^3.3",
    "fakerphp/faker": "^1.24",
    "guzzlehttp/guzzle": "^7.10"
  },
  "require-dev": {
    "laravel/breeze": "^1.21",
    "nunomaduro/collision": "^7.12",
    "spatie/laravel-ignition": "^2.9",
    "laravel/sail": "^1.48",
    "laravel/tinker": "^2.10"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
